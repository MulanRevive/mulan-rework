func 可变形参(...) {
    for 参数 : ... {
        print(typeof(参数))
    }
}
type Test {
    {}
}
可变形参(1, 2.3, '3', Test())
可变形参 = (a, b=3, ...) -> a*b*__varargs__
print(len(可变形参(1, 2, 3, 4)))
