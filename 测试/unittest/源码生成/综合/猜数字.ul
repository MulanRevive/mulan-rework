using sys, cmd, random


type 猜数字 : cmd.Cmd {
   {
    intro, prompt = tuple('我想了个 100 之内的数，猜猜是几？', '请猜吧: ')
    想的 = random.randrange(100)
  }

  func $default(行) {
    数 = int(行)
    if (数 > $想的) {
      println('太大了!')
    } elif (数 < $想的) {
      println('太小了!')
    } else {
      println('中了!')
      sys.exit()
    }
  }
}
猜数字().cmdloop()