using 库_常用字

拆字表 = {
"罐": ["缶", "雚"],
"蠢": ["春", "虫", "虫"],
"霸": ["雨", "革", "月"],
"露": ["雨", "路"],
"灌": ["氵", "雚"],
"镰": ["钅", "廉"]
}

部首 = "氵钅"

易拆 = []
for 字 in 拆字表 {
  各部分 = 拆字表[字]
  都常用 = true
  for 部分 in 各部分 {
    if !库_常用字.为常用字(部分) and 部首.find(部分) < 0{
      都常用 = false
    }
  }
  if 都常用 { 易拆.append(字) }
}
print(易拆)