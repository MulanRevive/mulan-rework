using 范围 in 测试.实用.规划.范围
using 点 in 测试.实用.规划.点
using 相等 in 测试.实用.通用.测试相关

type 占地 {
  func $占地(某范围, 原点) {
    $原点 = 原点

    横偏移 = 原点.横
    纵偏移 = 原点.纵
    $范围 = 范围(list(map(某点 -> [某点.横 + 横偏移, 某点.纵 + 纵偏移], 某范围.所有点)))
  }

  func $__repr__ {
    return "中心为`$原点`的`$范围`"
  }
}

相等(范围([
    [1, 2],
    [1, 1], [2, 1]
  ]), 占地(范围([
    [0, 1],
    [0, 0], [1, 0]
  ]), 点(1, 1)).范围, "偏移")
