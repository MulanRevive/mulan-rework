using 相等 in 测试.实用.通用.测试相关

type 点 {
    func $点(横, 纵) {
      $横, $纵 = 横, 纵
    }

    operator == (另一点) {
      return $横 == 另一点.横 and $纵 == 另一点.纵
    }

    func __repr__(self) {
      return "(`$横`, `$纵`)"
    }
}

相等(0, 点(0, 1).横, "点横坐标")
相等(1, 点(0, 1).纵, "点纵坐标")
相等(true, 点(0, 1) == 点(0, 1), "点相等")
相等(false, 点(1, 1) == 点(0, 1), "点不相等")