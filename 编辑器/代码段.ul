
type 代码段 {
  func $代码段(词性, 内容, 行, 起, 止) {
    $词性 = 词性
    $内容 = 内容
    $行 = 行
    $起, $止 = 起, 止
  }

  func $开始 {
    return str($行) + "." + str($起)
  }

  func $结束 {
    return str($行) + "." + str($止)
  }

  func $__repr__ {
    return "[" + $词性 + " 行:" + str($行) + " 列:" + str($起) + "~" + str($止) + "]"
  }
}
